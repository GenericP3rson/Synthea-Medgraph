drop job loadConditions

Create loading job loadConditions for graph @graphname@ {

    define filename f1;

    load f1
        to vertex Conditions values ($4, $5),
        to vertex SnomedCode values ($4, $5),
        to edge CONDITION_CODE values ($4, $4),
        to edge PATIENT_HAS_CONDITION values ($4, $2, $0, $1),
        to EDGE ENCOUNTER_FOR_CONDITION values ($4, $3)
        using header="true", separator=",";
}
