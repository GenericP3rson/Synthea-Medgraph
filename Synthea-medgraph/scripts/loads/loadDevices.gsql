drop job loadDevices

create loading job loadDevices for graph MedGraph {

    define filename f1;

    load f1 to Vertex Device values ($6, $5) using header="true";
    load f1 to Vertex SnomedCode values ($4, $5) using header="true";
    load f1 to EDGE PATIENT_HAS_DEVICE values ($6, $2, $0, $1) using header="true";
    load f1 to EDGE DEVICE_CODE values ($6, $4) using header="true";
    load f1 to EDGE ENCOUNTER_FOR_DEVICE values ($6, $3) using header="true";

}
