drop job loadLocations

create loading job loadLocations for graph @graphname@ {

    define filename f1;

    load f1
        to vertex State values ($3),
        to vertex County values ($5),
        to vertex City values ($2),
        to edge STATE_HAS_COUNTY values ($3, $5),
        to edge COUNTY_HAS_CITY values ($5, $2)
        using header="true", separator=",";
}
