drop job loadLocations

create loading job loadLocations for graph @graphname@ {

    define filename f1;

    load f1
        to vertex _State values ($3),
        to vertex _County values ($5),
        to vertex _City values ($2),
        to edge _STATE_HAS_COUNTY values ($3, $5),
        to edge _COUNTY_HAS_CITY values ($5, $2)
        using header="true", separator=",";
}
